    <Name>PLACEHOLDER_CODED_DOORS</Name>
    <Curiosity>PLACEHOLDER_VOLCANO_SUMMIT_CURIOSITY</Curiosity>

    <RumorFact>
      <ID>PLACEHOLDER_CODED_DOORS_RUMOR</ID>
      <SourceID>PLACEHOLDER_SCOUT_CODE</SourceID>
      <Text>[Reveal when we find any scout code] Wow I found a code</Text>
    </RumorFact>
    <RumorFact>
      <ID>PLACEHOLDER_CODED_DOORS_RUMOR2</ID>
      <SourceID>PLACEHOLDER_KARVI_MESSAGE</SourceID>
      <Text>[Reveal when we read the Karvi message] Wow I found a code</Text>
    </RumorFact>
